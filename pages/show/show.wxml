
<!-- title and book plane button -->
<view class='header-wrapper'>
  <button class='book-button'>Book</button>
  <text class='title'>{{model}}</text>
</view>

<!-- Image carousel -->
<view class='image-wrapper'>
  <image src="http://buzzghana.com/wp-content/uploads/2013/12/Airbus-319-Corporate-Jet-Owner-Vijay-Mallya-2.jpg"></image>
</view>

<!-- Details of jet section -->
<view class='details-wrapper'>
  <view class='details-model'>
    <text>{{model}}</text>
  </view>
  <view class='details-price'>
    <text>jet.price</text>
  </view>
  <view>
    <view class='amenities'>
      <text>jet.description</text>
    </view>
  </view>
</view>

<!-- Reviews render -->
<view class='reviews-header'>Reviews</view>
<view class="message">
  <image class="avatar-large" src="https://kitt.lewagon.com/placeholder/users/gabriel-dehan"></image>
  <view class='body-wrapper'>
    <view class="message-name">
      <text class='name-date'>Gab Dehan  |  10/07/17</text>
      <text>This was a great jet to ride in. WIll show to all my friends</text> 
    </view>
  </view>
</view>



