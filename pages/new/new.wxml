<!--pages/list/list.wxml-->
<view class="wrapper">
<view class="row">
  <view class="details">Details</view>
  <view class="confirm">Confirm</view>
</view>

<view class="addphotos">
  <view>
   <image src='{{tempFilePaths}}' bindtap='buttonClicked'></image> 
  </view>
  <button class="add" form-type="submit">Add Photos</button>
</view>

  <!--input.wxml-->

  <form bindsubmit="bindFormSubmit">
  
  <view class="section">
    <input name='model' placeholder='model'></input>
    <input name='manufactory' placeholder='manufactory'></input>
    <input name='location' placeholder='parking airport'></input>
     <input name='capacity_of_passengers' placeholder='Max capacity of passagers'></input>   
  </view> 

  <!-- Refer to the Github repository above if you want the complete form -->
  <button class="bottom" form-type="submit">Next</button>
 </form>
 </view>






